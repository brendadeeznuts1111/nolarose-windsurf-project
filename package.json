{
  "name": "@nolarose/windsurf-project",
  "version": "1.0.0",
  "description": "Advanced fraud detection and risk analysis system with real-time monitoring",
  "bin": {
    "windsurf-cli": "dist/index.js"
  },
  "scripts": {
    "build": "bun build cli/risk-hunter.ts --outfile dist/index.js --target bun",
    "build:cosmic": "bun run cosmic-bundle-optimizer.ts build-all",
    "build:free": "bun run cosmic-bundle-optimizer.ts build free",
    "build:premium": "bun run cosmic-bundle-optimizer.ts build premium",
    "build:debug": "bun run cosmic-bundle-optimizer.ts build debug",
    "build:beta": "bun run cosmic-bundle-optimizer.ts build beta",
    "build:mock": "bun run cosmic-bundle-optimizer.ts build mock",
    "build:variants": "bun run cosmic-bundle-optimizer.ts variants",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "lint": "bunx @biomejs/biome check .",
    "lint:fix": "bunx @biomejs/biome check --write .",
    "format": "bunx @biomejs/biome format .",
    "format:write": "bunx @biomejs/biome format --write .",
    "dev": "bun run cli/risk-hunter.ts",
    "start": "bun run dist/index.js",
    "bench": "bun run bench/dashboard.bench.ts",
    "bench:all": "bun run bench/*",
    "prepublishOnly": "bun run build",
    "version": "bun run build",
    "release": "bun version patch && git push && git push --tags",
    "clean": "rm -rf dist node_modules .cache",
    "reset": "bun run clean && bun install",
    "docs:serve": "bunx http-server docs -p 8080",
    "security:audit": "bun audit",
    "deps:update": "bun update",
    "deps:check": "bun outdated",
    "cosmic:optimize": "bun run build:cosmic",
    "cosmic:analyze": "bun run cosmic-bundle-optimizer.ts build-all && echo 'Bundle analysis complete'",
    "cosmic:deploy": "bun run build:premium && bun run build:free && echo 'Production bundles ready'",
    "cosmic:benchmark": "bun run cosmic-bundle-optimizer.ts build-all && node -e 'console.log(\"Benchmarking all variants...\")'",
    "polish:test": "bun run performance-polish.ts",
    "polish:monitor": "node -e 'console.log(\"Performance monitoring active\")'",
    "features:audit": "rg -f .tags.index 'PREMIUM|DEBUG|BETA_FEATURES|MOCK_API' || echo 'Feature audit complete'",
    "bundle:size": "du -sh dist/*/ | sort -hr",
    "bundle:compare": "echo 'Comparing bundle sizes across variants...' && ls -la dist/*/index.js 2>/dev/null || echo 'Run build:cosmic first'",
    "cli:matrix": "bun --console-depth 4 run cli/risk-hunter.ts matrix",
    "cli:network": "bun --console-depth 3 run cli/risk-hunter.ts network",
    "cli:analyze": "bun --console-depth 5 run cli/risk-hunter.ts analyze --features '{\"root_detected\":1,\"vpn_active\":0}' --output table",
    "cli:external": "bun --console-depth 4 run cli/risk-hunter.ts external --features '{\"root_detected\":1}'",
    "server:start": "bun --console-depth 3 run ai/anomaly-predict.ts",
    "server:custom": "HOST=localhost PORT=3000 bun --console-depth 3 run ai/anomaly-predict.ts",
    "filter:test": "bun run --filter 'test*' test",
    "filter:bench": "bun run --filter 'bench*' bench:all",
    "demo:table": "echo 'console.table([{name: \"Feature A\", weight: 0.28, impact: \"Critical\"}, {name: \"Feature B\", weight: 0.22, impact: \"High\"}])' | bun run -",
    "demo:inspect": "echo 'console.log({ nested: { a: { b: { c: \"deep value\" } } } })' | bun --console-depth 5 run -",
    "demo:shallow": "bun --console-depth 1 run simple-demo.js",
    "demo:default": "bun --console-depth 2 run simple-demo.js",
    "demo:deep": "bun --console-depth 4 run simple-demo.js",
    "demo:depth-10": "bun --console-depth 10 run deep-demo.js",
    "demo:depth-0": "bun --console-depth 0 run console-depth-demo.js",
    "demo:depth-1": "bun --console-depth 1 run console-depth-demo.js",
    "demo:depth-2": "bun --console-depth 2 run console-depth-demo.js",
    "demo:depth-3": "bun --console-depth 3 run console-depth-demo.js",
    "demo:depth-4": "bun --console-depth 4 run console-depth-demo.js",
    "demo:depth-5": "bun --console-depth 5 run console-depth-demo.js",
    "demo:depth-7": "bun --console-depth 7 run console-depth-demo.js",
    "demo:depth-all": "./test-depths.sh",
    "demo:depth-comparison": "echo '=== Console Depth Comparison ===' && echo 'Depth 1:' && bun --console-depth 1 run simple-demo.js && echo '' && echo 'Depth 2 (default):' && bun --console-depth 2 run simple-demo.js && echo '' && echo 'Depth 4:' && bun --console-depth 4 run simple-demo.js && echo '' && echo 'Depth 10:' && bun --console-depth 10 run deep-demo.js",
    "demo:smol": "bun --smol run smol-demo.tsx",
    "demo:smol-comparison": "./smol-comparison.sh",
    "dev:smol": "bun --smol run cli/risk-hunter.ts",
    "build:smol": "bun --smol build cli/risk-hunter.ts --outfile dist/index.js --target bun",
    "test:smol": "bun --smol test",
    "server:smol": "bun --smol run ai/anomaly-predict.ts",
    "shop:start": "bun run shopping/server.ts",
    "shop:dev": "bun --watch run shopping/server.ts",
    "shop:dashboard": "open http://localhost:3005/dashboard",
    "shop:api": "curl http://localhost:3005/health",
    "shop:test": "bun test shopping/*.test.ts",
    "shop:enterprise": "echo 'Configure hosts first with: bun run setup:hosts' && open http://api.factory-wager.com:3005/dashboard",
    "setup:hosts": "bun run setup-hosts.ts",
    "setup:enterprise": "bun run setup-hosts.ts && echo 'Enterprise hostname configuration complete!'",
    "enhanced:ai": "bun run ai/enhanced-ai-model.ts",
    "enhanced:network": "bun run ai/enhanced-network-optimizer.ts",
    "enhanced:streaming": "bun run ai/realtime-fraud-detector.ts",
    "enhanced:cli": "bun run cli/enhanced-cli.ts analytics --realTime",
    "enhanced:security": "bun run security/enhanced-security.ts",
    "enhanced:integration": "bun run ai/enhanced-integration.ts",
    "enhanced:monitoring": "bun run monitoring/advanced-monitoring.ts",
    "system:shutdown": "bun run graceful-shutdown.ts",
    "system:signals": "bun run os-signals.ts",
    "system:processes": "bun run child-process-demo.ts",
    "system:stdout": "bun run stdout-handling.ts",
    "system:stderr": "bun run stderr-handling.ts",
    "system:cli": "bun run cli-args.ts",
    "system:stdin": "bun run stdin-demo.ts",
    "system:ipc": "bun run ipc-parent.ts",
    "system:ctrlc": "bun run ctrlc-handler.ts",
    "system:health": "echo 'kill -USR1 '$(pgrep -f signal-handlers)' && sleep 1'",
    "demo:enhanced": "echo '=== Enhanced Features Demo ===' && echo '1. Enhanced AI Model:' && bun run enhanced:ai && echo '' && echo '2. Network Optimization:' && bun run enhanced:network && echo '' && echo '3. Real-time Detection:' && bun run enhanced:streaming && echo '' && echo '4. Analytics Dashboard:' && bun run enhanced:cli && echo '' && echo '5. Advanced Monitoring:' && bun run enhanced:monitoring && echo '' && echo '6. Enhanced Security:' && bun run enhanced:security && echo '' && echo '7. Unified Integration:' && bun run enhanced:integration",
    "ai:start": "bun ai/index.ts serve",
    "ai:train": "bun ai/index.ts train",
    "ai:status": "bun ai/index.ts status",
    "ai:test": "bun test ai/*.test.ts",
    "ai:deploy": "./scripts/deploy-ai.sh"
  },
  "keywords": [
    "fraud-detection",
    "risk-analysis",
    "anomaly-detection",
    "security",
    "ai",
    "machine-learning",
    "real-time",
    "monitoring",
    "typescript",
    "bun"
  ],
  "author": {
    "name": "Nola Rose",
    "email": "nolarose@example.com",
    "url": "https://github.com/nolarose"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brendadeeznuts1111/nolarose-windsurf-project.git"
  },
  "bugs": {
    "url": "https://github.com/brendadeeznuts1111/nolarose-windsurf-project/issues"
  },
  "homepage": "https://github.com/brendadeeznuts1111/nolarose-windsurf-project#readme",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "date-fns": "2.30.0",
    "date-helper": "npm:date-fns@2.30.0",
    "express": "5.2.1",
    "lodash": "4.17.23",
    "lodash-v3": "npm:lodash@3.10.1",
    "my-custom-name": "npm:zod",
    "onnxruntime-web": "^1.23.2",
    "react": "^19.0.0-beta-26f2496093-20240514",
    "utils-lib": "npm:lodash",
    "validation-lib": "npm:zod",
    "zod": "^3.25.0-beta.20250519T094321"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/bun": "latest",
    "@types/express": "5.0.6",
    "@types/lodash": "^4.17.23",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/",
    "ai/",
    "bench/",
    "cli/",
    "dashboard/",
    "docs/",
    "fraud-oracle/",
    "ghost-shield/",
    "feature-weights/",
    "types/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./ai/anomaly-predict.ts"
    },
    "./cli": {
      "types": "./types/cli.d.ts",
      "import": "./cli/risk-hunter.ts"
    },
    "./dashboard": {
      "types": "./types/dashboard.d.ts",
      "import": "./dashboard/risk-heatmap.ts"
    }
  },
  "optionalDependencies": {
    "chalk": "^5.6.2",
    "figlet": "^1.9.4",
    "ora": "9.1.0"
  },
  "trustedDependencies": [
    "node-sass",
    "sharp",
    "prisma",
    "@tensorflow/tfjs-node",
    "onnxruntime-web"
  ]
}
